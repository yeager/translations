msgctxt "#30000"
msgid "Default length"
msgstr "Standardlängd"

msgctxt "#30001"
msgid "Used if specified file does not provide the length."
msgstr "Används om den angivna filen inte innehåller någon längd."

msgctxt "#30002"
msgid "unused"
msgstr "oanvänd"

msgctxt "#30003"
msgid "{0:d} s"
msgstr "{0:d} s"

msgctxt "#30004"
msgid "Default fade out time"
msgstr "Standardtid för uttoning"

msgctxt "#30005"
msgid "Used if specified file does not provide the length and fade."
msgstr "Används om den angivna filen inte innehåller längd och uttoning."

msgctxt "#30006"
msgid "unused"
msgstr "oanvänd"

msgctxt "#30007"
msgid "{0:d} ms"
msgstr "{0:d} ms"

msgctxt "#30008"
msgid "Suppress opening silence"
msgstr "Ta bort tystnaden vid öppning"

msgctxt "#30009"
msgid "Some files start with silent data, this prevents it and looks for the beginning."
msgstr "Vissa filer börjar med tysta data, detta förhindrar det och letar efter början."

msgctxt "#30010"
msgid "Suppress end silence"
msgstr "Ta bort tystnaden i slutet"

msgctxt "#30011"
msgid "Some files end with silent data, this prevents it and looks for the end."
msgstr "Vissa filer slutar med tysta data, detta förhindrar det och letar efter slutet."

msgctxt "#30012"
msgid "Second of silence to check"
msgstr "Sekunder av tystnad att kontrollera"

msgctxt "#30013"
msgid "How many silent seconds are allowed before playback is stopped."
msgstr "Hur många tysta sekunder som tillåts innan uppspelningen stoppas."

msgctxt "#30014"
msgid "{0:d} s"
msgstr "{0:d} s"

msgctxt "#30015"
msgid "Direct output"
msgstr "Direkt utmatning"

msgctxt "#30016"
msgid "Enable direct (dry) output."
msgstr "Aktivera direkt (torr) utmatning."

msgctxt "#30017"
msgid "DSP emulation"
msgstr "DSP-emulering"

msgctxt "#30018"
msgid "Enable DSP emulation (for reverb, etc.)."
msgstr "Aktivera DSP-emulering (för reverb etc.)."

msgctxt "#30019"
msgid "Dynamic recompiler"
msgstr "Dynamisk omkompilator"

msgctxt "#30020"
msgid "Emulate DSP using dynamic recompiler."
msgstr "Emulera DSP med hjälp av dynamisk omkompilator."
