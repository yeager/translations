msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "Tillägg för rTorrent-kontroll"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Ett enkelt tillägg för interaktion med rTorrent via HTTP eller SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Kräver en rTorrent-instans med XMLRPC aktiverat."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Stöd för SCGI- och HTTP-protokoll.\n"
"Exempel: http://värd:port, scgi://värd:port, scgi:///sökväg/till/uttag"

msgctxt "#30004"
msgid "Downloading"
msgstr "Hämtar"

msgctxt "#30005"
msgid "Complete"
msgstr "Färdig"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Fjärrmappar (om rTorrent finns på en fjärrdator)"

msgctxt "#30080"
msgid "Connection"
msgstr "Anslutning"

msgctxt "#30081"
msgid "Remote"
msgstr "Fjärr"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Starta hämtning"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Stoppa hämtning"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Radera hämtning"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Ställ in prioritet: Hög"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Ställ in prioritet: Normal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Ställ in prioritet: Låg"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Ställ in prioritet: Inaktiv"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Ställ in prioritet: Hämta inte ner"

msgctxt "#30150"
msgid "Play File"
msgstr "Spela upp fil"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Den här filen har inte hämtats ner helt."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Är du säker på att du vill spela upp den?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Ta bort hämtning"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Är du säker på att du vill ta bort den här hämtningen?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Fel!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Det går inte att spela upp filen."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Anslutning misslyckades"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Kontrollera inställningarna..."

msgctxt "#30904"
msgid "Exit"
msgstr "Avsluta"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Något gick fel."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Ett XMLRPC-protokollfel inträffade. Skicka in en felrapport!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Ett XMLRPC-fel inträffade. Skicka in en felrapport!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Anslutning till rTorrent nekas. Kontrollera brandväggsinställningarna."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Anslutningen till rTorrent översteg tidsgräns. Kontrollera att värden är korrekt."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Ett oväntat fel inträffade."
